{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "printWidth": 80,
  "endOfLine": "lf",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "plugins": ["prettier-plugin-css-order"],
  "cssDeclarationSorterOrder": "smacss"
}
